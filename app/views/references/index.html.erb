<p id="notice"><%= notice %></p>

<h1>References</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Project</th>
      <th>Country city</th>
      <th>Address</th>
      <th>Address link</th>
      <th>Installer</th>
      <th>Installer link</th>
      <th>Owner</th>
      <th>Owner link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @references.each do |reference| %>
      <tr>
        <td><%= reference.tag %></td>
        <td><%= reference.project %></td>
        <td><%= reference.country_city %></td>
        <td><%= reference.address %></td>
        <td><%= reference.address_link %></td>
        <td><%= reference.installer %></td>
        <td><%= reference.installer_link %></td>
        <td><%= reference.owner %></td>
        <td><%= reference.owner_link %></td>
        <td><%= link_to 'Show', reference %></td>
        <td><%= link_to 'Edit', edit_reference_path(reference) %></td>
        <td><%= link_to 'Destroy', reference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reference', new_reference_path %>
