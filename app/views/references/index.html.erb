<p id="notice"><%= notice %></p>

<h1>References</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Customer</th>
      <th>Type</th>
      <th>Project</th>
      <th>Product</th>
      <th>Code</th>
      <th>Location</th>
      <th>Quantity</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @references.each do |reference| %>
      <tr>
        <td><%= reference.image %></td>
        <td><%= reference.customer %></td>
        <td><%= reference.type %></td>
        <td><%= reference.project %></td>
        <td><%= reference.product %></td>
        <td><%= reference.code %></td>
        <td><%= reference.location %></td>
        <td><%= reference.quantity %></td>
        <td><%= reference.year %></td>
        <td><%= link_to 'Show', reference %></td>
        <td><%= link_to 'Edit', edit_reference_path(reference) %></td>
        <td><%= link_to 'Destroy', reference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reference', new_reference_path %>
