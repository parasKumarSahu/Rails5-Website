<div class="container" style="margin-top:100px">
<% Family.all.each do |f| %>
	<div class="col-sm-6 col-sm-offset-4">
		<h2><%= f.name %></h2>
			<ul>
			<% f.products.each do |p|%>
		        <oi>
		        	<h3><%= link_to p.name, "/home/overview?product="+p.name%></h3>
		        </oi>
			<% end %>		
			</ul>
	</div>
<% end %>
</div>
