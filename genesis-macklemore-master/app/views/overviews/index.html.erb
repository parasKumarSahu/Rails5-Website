<p id="notice"><%= notice %></p>

<h1>Overviews</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @overviews.each do |overview| %>
      <tr>
        <td><%= overview.body %></td>
        <td><%= overview.product %></td>
        <td><%= link_to 'Show', overview %></td>
        <td><%= link_to 'Edit', edit_overview_path(overview) %></td>
        <td><%= link_to 'Destroy', overview, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Overview', new_overview_path %>
